					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": [],
				"lastPublishTime": "2022-03-23T22:24:14Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/ds_covidreporting_sa')]",
				"[concat(variables('factoryId'), '/datasets/ds_covidreporting_dl')]"
			]
